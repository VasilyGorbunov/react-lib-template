{
  "name": "react-lib-template",
  "version": "0.0.1",
  "private": false,
  "main": "dist/bundle.min.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "antd": "^5.18.0",
    "axios": "^1.7.2",
    "crypto-pro": "^2.3.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "react-resizable": "^3.0.5",
    "styled-components": "^6.1.11"
  },
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "rollup --config",
    "build-storybook": "build-storybook",
    "create-component": "node ./util/create-component",
    "test": "yarn lint",
    "lint": "yarn lint:js",
    "lint:js": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "pub:rc": "./bin/publish-rc.sh",
    "pub:patch": "yarn version --patch",
    "pub:minor": "yarn version --minor",
    "pub:major": "yarn version --major",
    "prepublish": "yarn test && yarn build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addon-controls": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.8",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.39",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-resizable": "^3.0.2",
    "@types/styled-components": "^5.1.25",
    "colors": "^1.4.0",
    "eslint-config-prettier": "^8.5.0",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "rollup": "^2.75.6",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.7.3",
    "typescript-styled-plugin": "^0.18.2",
    "webpack": "^5.73.0"
  },
  "peerDependencies": {
    "react": ">16.0.0",
    "react-dom": ">16.0.0"
  }
}
